{
    "name": "ressapp/ressapp",
    "description": "Offline-ready property management demo.",
    "type": "project",
    "require": {
        "php": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Framework\\": "framework/",
            "Illuminate\\": "framework/Illuminate/",
            "PHPUnit\\": "framework/PHPUnit/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php scripts/install_phpunit_runner.php"
        ],
        "post-update-cmd": [
            "php scripts/install_phpunit_runner.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
