{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "laravel/ui": "^4.6",
    "stancl/tenancy": "^3.9",
    "stripe/stripe-php": "^17.4",
    "laravel/tinker": "^2.10",
    "spatie/laravel-permission": "^6.0",
    "laravel/sanctum": "^4.0",
    "sentry/sentry-laravel": "^4.19"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^10.5",
    "symfony/css-selector": "^6.2",
    "symfony/dom-crawler": "^6.2",
    "laravel/breeze": "^2.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "php scripts/offline-optimize.php"
    ],
    "post-update-cmd": [
      "php scripts/offline-optimize.php"
    ],
    "pre-install-cmd": [
      "php scripts/composer-token.php"
    ],
    "pre-update-cmd": [
      "php scripts/composer-token.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
