		<script type="text/javascript">
			var renderCharts = () => {

				const $charts = $(".property-stats-chart");

				$charts.each((index, element) => {
					const chartData = JSON.parse(element.dataset.chartData);

					if(element.dataset.rendered) {
						return;
					}

					const { labels, datasets, backgroundColours } = chartData;

					if(!labels) {
						return;
					}

					const resolution = $("#hit-resolution").val();

					let timeUnit = "day";
					let majorEnabled = false;

					if(resolution === "h") {
						timeUnit = "hour";
						majorEnabled = true;
					}
					if(resolution === "m") {
						timeUnit = "month";
					}

					const data = {
						labels: labels,
						datasets: []
					};

					datasets.forEach((dataset, index) => {
						data.datasets.push({
							label: dataset.title,
							data: dataset.values,
							borderColor: backgroundColours[index],
						})
					});

					const config = {
						type: "line",
						data: data,
						options: {
							responsive: true,
							maintainAspectRatio: false,
							scales: {
								x: {
									type: "time",
									ticks: {
										font: (data) => {
											const font = { ...Chart.defaults.font };
											if(data.tick.major) {
												font.weight = "bold";
												font.color = "#000";
											}

											return font;
										},
										major: {
											enabled: majorEnabled
										}
									},
									time: {
										displayFormats: {
											hour: "HH:mm",
											day: "d MMM",
											month: "MMM yy",
										},
										unit: timeUnit
									}
								},
								y: {
									min: 0,
									suggestedMax: 10,
									ticks: {
										stepSize: 1
									}
								}
							}
						}
					};

					new Chart(element, config);
					element.dataset.rendered = "true";
				});
			};

			if(document.readyState !== "loading") {
				renderCharts();
			}
			else {
				document.addEventListener("DOMContentLoaded", () => renderCharts());
			}
		</script>

